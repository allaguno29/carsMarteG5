<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.controller.PaginaPrincipalController">
   <children>
      <VBox id="navigator" prefHeight="304.0" prefWidth="180.0">
         <children>
            <ImageView fx:id="btnImage" fitHeight="190.0" fitWidth="180.0" onMouseClicked="#mostrarPerfil" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../img/default.jpg" />
               </image>
            </ImageView>
            <Separator maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="0.5" prefWidth="200.0" />
            <Button fx:id="btnVendedor" alignment="BASELINE_CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#mostrarVendedor" prefHeight="45.0" prefWidth="180.0" text="Vendedor" textAlignment="CENTER" textOverrun="LEADING_ELLIPSIS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Separator maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="0.5" prefWidth="200.0" />
            <Button fx:id="btnComprador" alignment="BASELINE_CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#mostrarComprador" prefHeight="45.0" prefWidth="180.0" text="Comprador" textAlignment="CENTER" textOverrun="LEADING_ELLIPSIS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Separator maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="0.5" prefWidth="200.0" />
            <Separator maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="0.5" prefWidth="200.0" />
            <Button fx:id="salirbtn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#salir" prefHeight="45.0" prefWidth="180.0" text="Salir">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Separator orientation="VERTICAL" prefHeight="200.0" />
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="420.0">
         <center>
            <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <top>
                  <Text fill="#c72323" strokeType="OUTSIDE" strokeWidth="0.0" text="CARSMATE" textAlignment="CENTER" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="36.0" />
                     </font>
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="100.0" top="10.0" />
                     </BorderPane.margin>
                  </Text>
               </top>
               <center>
                  <VBox fx:id="Vcenter" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
               </center>
            </BorderPane>
         </center>
      </BorderPane>
   </children>
</HBox>
